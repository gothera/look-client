{
  "name": "look-client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand --watch",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "postinstall": "patch-package",
    "check-format": "npm run prettier -- --list-different",
    "check-types": "tsc",
    "check-build": "react-native bundle --entry-file index.js --bundle-output /dev/null",
    "validate": "npm-run-all --parallel check-types check-format check-build lint test"
  },
  "dependencies": {
    "@react-native-community/checkbox": "0.5.7",
    "@react-native-firebase/app": "^12.0.0",
    "@react-native-firebase/messaging": "^12.0.0",
    "axios": "0.19.1",
    "dayjs": "^1.10.3",
    "formik": "2.1.5",
    "patch-package": "6.2.2",
    "postinstall-postinstall": "2.1.0",
    "react": "16.13.1",
    "react-native": "0.63.2",
    "react-native-calendars": "https://github.com/wix/react-native-calendars.git",
    "react-native-date-picker": "3.2.3",
    "react-native-fast-image": "8.3.2",
    "react-native-gesture-handler": "1.6.0",
    "react-native-image-crop-picker": "0.32.2",
    "react-native-image-viewing": "^0.2.0",
    "react-native-indicators": "0.17.0",
    "react-native-keyboard-accessory": "0.1.11",
    "react-native-keyboard-aware-scroll-view": "0.9.2",
    "react-native-keyboard-spacer-view": "0.0.3",
    "react-native-keychain": "6.1.1",
    "react-native-linear-gradient": "2.5.6",
    "react-native-maps": "0.27.1",
    "react-native-modalize": "2.0.6",
    "react-native-navigation": "7.4.0",
    "react-native-navigation-hooks": "6.2.0",
    "react-native-picker-select": "7.0.0",
    "react-native-reanimated": "^1.13.1",
    "react-native-scrollable-tab-view": "^1.0.0",
    "react-native-share": "5.1.0",
    "react-native-status-bar-height": "2.5.0",
    "react-native-svg": "12.1.0",
    "react-native-swiper": "1.6.0",
    "react-native-tab-view": "2.15.1",
    "react-redux": "7.2.0",
    "redux": "4.0.5",
    "redux-persist": "6.0.0",
    "redux-persist-filesystem-storage": "2.1.0",
    "redux-persist-transform-filter": "0.0.20",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "rn-fetch-blob": "0.12.0"
  },
  "devDependencies": {
    "@babel/cli": "7.10.4",
    "@babel/core": "7.10.4",
    "@babel/plugin-proposal-decorators": "7.10.4",
    "@babel/plugin-proposal-optional-catch-binding": "7.10.4",
    "@babel/runtime": "7.10.4",
    "@react-native-community/eslint-config": "2.0.0",
    "@testing-library/jest-native": "3.1.0",
    "@testing-library/react-native": "6.0.0",
    "@types/jest": "25.2.3",
    "@types/react-native": "0.63.2",
    "@types/react-native-calendars": "1.20.10",
    "@types/react-native-indicators": "0.16.0",
    "@types/react-native-share": "^3.3.1",
    "@types/react-redux": "7.1.9",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.1.0",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-transform-inline-environment-variables": "0.4.3",
    "babel-plugin-transform-remove-console": "6.9.4",
    "eslint": "7.5.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.20.3",
    "eslint-plugin-react-native": "3.8.1",
    "eslint-plugin-standard": "4.0.1",
    "husky": "4.2.5",
    "is-ci-cli": "2.1.2",
    "jest": "25.1.0",
    "jest-watch-typeahead": "0.6.0",
    "lint-staged": "10.2.11",
    "metro-react-native-babel-preset": "0.61.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "react-test-renderer": "16.13.1",
    "redux-devtools-extension": "2.13.8",
    "ts-jest": "26.1.3",
    "typescript": "3.9.7",
    "utility-types": "3.10.0"
  },
  "lint-staged": {
    "*.+(js|ts|tsx)": [
      "eslint",
      "jest --findRelatedTests"
    ],
    "**/*.+(js|json|ts|tsx|md)": [
      "prettier --write"
    ]
  }
}
